{
  "productName": "CubeShuffle",
  "version": "0.1.0",
  "identifier": "com.philipborg.cubeshuffle",
  "build": {
    "frontendDist": "websrc",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "cd ../cube_shuffle-wasm && trunk serve",
    "beforeBuildCommand": "rm -rf websrc && mkdir -p websrc && cd ../cube_shuffle-wasm && trunk build --release --dist ../src-tauri2/websrc"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "CubeShuffle",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "https://github.com/philipborg/CubeShuffle/blob/master/LICENSE",
    "category": "Utility",
    "shortDescription": "",
    "longDescription": ""
  },
  "plugins": {}
}
